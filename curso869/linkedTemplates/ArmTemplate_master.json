{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nombre de instancia de Data Factory",
			"defaultValue": "curso869"
		},
		"LabsLabs_accountKey": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"accountKey\"de \"LabsLabs\""
		},
		"SQLPoolPractice_password": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"password\"de \"SQLPoolPractice\""
		},
		"LabsLabs_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://labs4899.dfs.core.windows.net/"
		},
		"SQLPoolPractice_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "labs4899.sql.azuresynapse.net"
		},
		"SQLPoolPractice_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sqlpool"
		},
		"SQLPoolPractice_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladminuser"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI de la cuenta de almacenamiento que contiene las plantillas de ARM vinculadas."
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Token de SAS para la cuenta de almacenamiento que contiene las plantillas de ARM vinculadas"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "curso869_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LabsLabs_accountKey": {
						"value": "[parameters('LabsLabs_accountKey')]"
					},
					"SQLPoolPractice_password": {
						"value": "[parameters('SQLPoolPractice_password')]"
					},
					"LabsLabs_properties_typeProperties_url": {
						"value": "[parameters('LabsLabs_properties_typeProperties_url')]"
					},
					"SQLPoolPractice_properties_typeProperties_server": {
						"value": "[parameters('SQLPoolPractice_properties_typeProperties_server')]"
					},
					"SQLPoolPractice_properties_typeProperties_database": {
						"value": "[parameters('SQLPoolPractice_properties_typeProperties_database')]"
					},
					"SQLPoolPractice_properties_typeProperties_userName": {
						"value": "[parameters('SQLPoolPractice_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}